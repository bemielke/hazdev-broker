{
	"Type":"ConsumerClient",

	# The file extension to use
	"FileExtension":"default",

	# the file name 
	"FileName":"default",

	# the directory to write output messages (and optionally heartbeat files)
	"OutputDirectory":"./out",

	# The maximum number of messages in a file
	"MessagesPerFile":10,

	# The time (in seconds) before writing any pending messages to file
	"TimePerFile":15,

	# The interval to expect heartbeat messages, -1 to disable
	"HeartbeatInterval":120,

	# whether to write heartbeat message file(s) to the OutputDirectory,
	# false to disable
	"WriteHeartbeatFile":true,

	# The hazdev broker configuration
	"HazdevBrokerConfig": {
		"Type":"ConsumerConfig",
		"Properties":{
			# the id of this consumer client
			"client.id":"consumer.example",

			# the group this consumer client belongs to (usually same as 
			# client.id)
			"group.id":"default.consumer",

			# the list of server(s) making up the kakfa broker cluster
			"bootstrap.servers":"localhost:9092",

			# whether to automatically commit / retrieve client offsets (last 
			# recieved position)
			"enable.auto.commit":"true"
		}
	},

	# The list of topics to subsribe to
	"TopicList":["test"],

	# Logging configuration
	"Log4JConfigFile":"consumerclient.log4j.properties"
}
